
// This is a JavaScript file that lists all the datatypes in JavaScript

// 1. String
let str = "I am happy to join PW Skills FSWD Course";

// 2. Number
// i) Integer
let integerNum = 9;

// ii) Floating value
let floatNum = 10.2;

// iii) Infinity and -Infinity
let positiveInfinity = Infinity;
let negativeInfinity = -Infinity;

// iv) NaN (Not a Number)
let nanValue = NaN;

// 3. BigInt
let bigIntNum = 1024n;

// 4. Boolean
let isTrue = true;
let isFalse = false;

// 5. Undefined
let undefinedValue;

// 6. Null
let nullValue = null;

// 7. Symbol
let symbolValue = Symbol("PW Skills");

// 8. Objects
// i) Array
let arrayValue = [1, 2, "PW"];

// Printing the examples and their types
console.log(typeof str, str);
console.log(typeof integerNum, integerNum);
console.log(typeof floatNum, floatNum);
console.log(typeof positiveInfinity, positiveInfinity);
console.log(typeof negativeInfinity, negativeInfinity);
console.log(typeof nanValue, nanValue);
console.log(typeof bigIntNum, bigIntNum);
console.log(typeof isTrue, isTrue);
console.log(typeof isFalse, isFalse);
console.log(typeof undefinedValue, undefinedValue);
console.log(typeof nullValue, nullValue);
console.log(typeof symbolValue, symbolValue);
console.log(typeof arrayValue, arrayValue);
