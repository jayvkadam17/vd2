// Valid variables
let name = "PW Skills";
console.log(name); // OUTPUT: PW Skills

let iAmHappy = true;

// Invalid Variables
let name1 = "PW Skills";
console.log(iname); // OUTPUT: ReferenceError: iname is not defined

let var = 13;
console.log(var); // OUTPUT: SyntaxError: Unexpected token 'var'
